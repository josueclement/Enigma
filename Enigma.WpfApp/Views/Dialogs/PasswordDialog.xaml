<UserControl x:Class="Enigma.WpfApp.Views.Dialogs.PasswordDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Enigma.WpfApp.Views.Dialogs"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
             
    <Grid VerticalAlignment="Center">
        <StackPanel>
            <Label Content="Private key password:" />
            <ui:PasswordBox x:Name="KeyPassword"
                            PasswordChanged="KeyPassword_OnPasswordChanged"/>
        </StackPanel>
    </Grid>
</UserControl>
