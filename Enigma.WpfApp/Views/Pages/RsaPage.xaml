<controls:NavigationPageBase x:Class="Enigma.WpfApp.Views.Pages.RsaPage"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:local="clr-namespace:Enigma.WpfApp.Views.Pages"
                             xmlns:controls="clr-namespace:Enigma.WpfApp.Controls"
                             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
                             mc:Ignorable="d"
                             d:DataContext="{d:DesignInstance local:RsaPage}">
    <DockPanel Margin="10">
        <StackPanel DockPanel.Dock="Top"
                    Orientation="Horizontal">
            <ui:Button Content="Generate key pair"
                       Appearance="Primary"
                       Command="{Binding ViewModel.GenerateKeyPairCommand}"/>
        </StackPanel>
        <Grid>
            
        </Grid>
    </DockPanel>
</controls:NavigationPageBase>
